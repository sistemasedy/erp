<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <!-- QWeb Reports -->
        <record id="account_invoices_emproduct" model="ir.actions.report">
            <field name="name"> Factura Calro </field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">em_product.account_claro_report</field>
            <field name="report_file">em_product.account_claro_report</field>
            <field name="print_report_name">'Invoice - %s' % (object.name).replace('/', '')</field>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_type">report</field>
        </record>

        

        <!-- report action  -->
        <record id="action_tax_invoice_report" model="ir.actions.report">
            <field name="name"> Factura Credito Fiscal </field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">em_product.account_move_report</field>
            <field name="report_file">em_product.account_move_report</field>
            <field name="print_report_name">'Invoice - %s' % (object.name).replace('/', '')</field>
            <field name="binding_model_id" ref="account.model_account_move"/>
            <field name="binding_type">report</field>
        </record>


      

        
    </data>
</odoo>
     

