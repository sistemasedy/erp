<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="project_torre_demo" model="project.project">
            <field name="name">Torre 15 Niveles - Demo</field>
            <field name="alias_name">torre15</field>
            <field name="date_start" eval="(DateTime.now() - relativedelta(months=6)).strftime('%Y-%m-%d')"/>
            <field name="user_id" ref="base.user_admin"/> <field name="active" eval="True"/>
        </record>

        <record id="project_plaza_comercial" model="project.project">
            <field name="name">Plaza Comercial Los Robles</field>
            <field name="alias_name">plazarobles</field>
            <field name="date_start" eval="(DateTime.now() - relativedelta(months=3)).strftime('%Y-%m-%d')"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="active" eval="True"/>
        </record>

        <record id="project_villa_turistica" model="project.project">
            <field name="name">Complejo de Villas "Costa Serena"</field>
            <field name="alias_name">villascs</field>
            <field name="date_start" eval="(DateTime.now() - relativedelta(months=1)).strftime('%Y-%m-%d')"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>