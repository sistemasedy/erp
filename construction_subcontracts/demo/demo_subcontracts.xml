<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Partner de demostración: Ajustero -->
        <record id="partner_ajustero_demo" model="res.partner">
            <field name="name">Andamios del Caribe S.R.L.</field>
            <field name="supplier_rank">1</field>
            <field name="phone">809-555-1234</field>
            <field name="email">info@andamiosdelcaribe.com</field>
        </record>
        
        <!-- Proyecto de demostración -->
        <record id="project_torre_demo" model="project.project">
            <field name="name">Torre 15 Niveles - Demo</field>
        </record>
        
        <!-- Subcontrato de demostración -->
        <record id="subcontract_demo_1" model="construction.subcontract">
            <field name="name">SC-DEMO-001</field>
            <field name="partner_id" ref="partner_ajustero_demo"/>
            <field name="project_id" ref="project_torre_demo"/>
            <field name="work_description">Instalación de andamios metálicos Niveles 3-7</field>
            <field name="measurement_unit">m2</field>
            <field name="unit_price">250.00</field>
            <field name="estimated_quantity">500.0</field>
            <field name="guarantee_percentage">20.0</field>
            <field name="state">active</field>
        </record>
        
    </data>
</odoo>