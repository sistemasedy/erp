<odoo>
    <data>
      <template id="button_acction">
        <div class="box box-primary">
          <div class="box-header">
            <i class="ion ion-clipboard"></i>
            <div class="mostrar_button" id="button_test">
              <div class="btn_clas" >
                <button id="mostrar_button" class="btn btn-primary" type="button" role="button">
                  <i class="fa fa-share-alt" role="img"/>
                </button>

              </div>
            </div>

            <h3 class="box-title">List of custom</h3>

            <div class="box-tools pull-right">
              <ul class="pagination pagination-sm inline">
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                                          
              </ul>
            </div>
          </div>
          <!-- /.box-header -->
          <div class="box-body">
              <div class="row">
                  <li t-foreach="todo_task" t-as="object">
                    <div class="info-box-content">
                      <!-- small box -->
                      <div class="small-box bg-aqua">
                        <div class="inner">
                          <h3><t t-esc="object.name"/></h3>

                          <p>New Orders</p>
                        </div>
                        <div class="icon">
                          <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                      </div>
                    </div>
                  </li>
              </div>
          </div>
        </div>
      </template>

      <template id="sider">
        <div class="box box-primary">
          <div class="box-header">
            <h3 class="box-title">List of Partner</h3>
          </div>
          <!-- /.box-header -->
          <div class="box-body">
            <div class="row">
              <div class="info-box-content">
                  <p>hola</p>
              </div>
            </div>
            <div class="row">
                <li t-foreach="todo_task" t-as="object">
                  <div class="info-box-content">
                    <div class="callout callout-info">
                      <h3><t t-esc="object.name"/></h3>

                      <p>Add</p>
                    </div>
                  </div>
                </li>
            </div>
          </div>
        </div>
      </template>



      <template id="my_snnipets">
        <section class="s_title">
          <div class="box box-primary">
            <div class="box-header">
              <h3 class="box-title">List of Partner</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <div class="row">
                <div class="info-box-content">
                    <p>hola</p>
                </div>
              </div>
              <div class="row">
                  <li >
                    <div class="info-box-content">
                      <div class="callout callout-info">
                        <h3>Testing snippet</h3>

                        <p>Add</p>
                      </div>
                    </div>
                  </li>
              </div>
            </div>
          </div>
        </section>
      </template>


      <template id="products_snnipets">
        <section class="s_title">
          <div id="products">
          </div>
        </section>
      </template>   
         
    </data>
</odoo>